{
	"name": "uuid-generator",
	"displayName": "uuid-generator",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Formatters",
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.uuid-generator-activate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.uuid-generator-activate",
				"title": "Hello World"
			}
		],
		"keybindings": [
			{
				"command": "extension.uuid-generator-activate",
				"key": "ctrl+alt+O",
				"mac": "cmd+shift+O"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/uuid": "^9.0.7",
		"@types/vscode": "^1.39.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"uuid": "^9.0.1"
	}
}
